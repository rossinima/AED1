#include <stdio.h>
 
int main() {
 
    double i, n1, n2, n3, n4, n5, n6, media;
    double soma = 0; 
    double positivos = 0;
    scanf("%lf %lf %lf %lf %lf %lf", &n1, &n2, &n3, &n4, &n5, &n6);
        if(n1 > 0){
            positivos++;
            soma = soma + n1;
        }
        if(n2 > 0){
            positivos++;
            soma = soma + n2;
        }
        if(n3 > 0){
            positivos++;
            soma = soma + n3;
        }
        if(n4 > 0){
            positivos++;
            soma = soma + n4;
        }
        if(n5 > 0){
            positivos++;
            soma = soma + n5;
        }
        if(n6 > 0){
            positivos++;
            soma = soma + n6;
        }
    media = soma/positivos;
    printf("%.0lf valores positivos\n", positivos);
    printf("%.1lf\n", media);
    return 0;
}
