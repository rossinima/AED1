#include <stdio.h>
#include <math.h>
 
int main() {
 
    float x1, x2, y1, y2, deltaX, deltaY, distancia;
    scanf("%f %f", &x1, &y1);
    scanf("%f %f", &x2, &y2);
    deltaX = (x2 - x1);
    deltaY = (y2 - y1);
    distancia = sqrt(pow(deltaX, 2) + pow(deltaY,2));
    printf("%.4f\n", distancia);
 
    return 0;
}
