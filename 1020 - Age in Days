#include <stdio.h>
 
int main() {
 
    int numero;
    float ano, mes, dia;
    scanf("%d", &numero);
    ano = (numero/365);
    mes = (numero % 365)/30;
    dia = (numero % 365)%30;
    printf("%.0f ano(s)\n", ano);
    printf("%.0f mes(es)\n", mes);
    printf("%0.f dia(s)\n", dia);
    
    return 0;
}
