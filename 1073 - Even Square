#include <stdio.h>
#include <math.h>
 
int main() {
 
    int n, i, quadrado;
    scanf("%d", &n);
    if(n % 2 == 0){
        for(i = 2; i <= n; i+=2){
            quadrado = pow(i,2);
            printf("%d^2 = %d\n", i, quadrado);
        }
    }
    else{
        for(i = 2; i < n; i = i + 2){
            quadrado = pow(i,2);
            printf("%d^2 = %d\n", i, quadrado);
        }
    }
    
 
    return 0;
}
