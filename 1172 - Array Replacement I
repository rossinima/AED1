#include <stdio.h>
 
int main() {
 
    int array[10];
    
    for(int i = 0; i < 10; i++){
        scanf("%d", &array[i]);
    }
    
    for(int j = 0; j < 10; j++){
        if(array[j] <= 0){
            array[j] = 1;
        }
        printf("X[%d] = %d\n", j, array[j]);
    }
 
    return 0;
}
