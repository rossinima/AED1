#include <stdio.h>
 
int main() {
 
    int i, j;
    for(i = 1, j = 7; j >= 5; j--, i){
        printf("I=%d J=%d\n", i, j);
    }
    for(i = 3, j = 9; j >= 7; j--, i){
        printf("I=%d J=%d\n", i, j);
    }
    for(i = 5, j = 11; j >= 9; j--, i){
        printf("I=%d J=%d\n", i, j);
    }
    for(i = 7, j = 13; j >= 11; j--, i){
        printf("I=%d J=%d\n", i, j);
    }
    for(i = 9, j = 15; j >= 13; j--, i){
        printf("I=%d J=%d\n", i, j);
    }
    return 0;
}
