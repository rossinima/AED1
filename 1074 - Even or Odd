#include <stdio.h>
 
int main() {
 
    int n, i, valor;
    scanf("%d", &n);
    for(i = 0; i < n; i++){
        scanf("%d", &valor);
        if(valor == 0){
            printf("NULL\n");
        }
        else if(valor > 0 && valor % 2 == 0){
            printf("EVEN POSITIVE\n");
        }
        else if(valor < 0 && valor % 2 == 0){
            printf("EVEN NEGATIVE\n");
        }
        else if(valor > 0 && valor % 2 != 0){
            printf("ODD POSITIVE\n");
        }
        else if(valor < 0 && valor % 2 != 0){
            printf("ODD NEGATIVE\n");
        }
    }
 
    return 0;
}
